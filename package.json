{
  "name": "music_project",
  "version": "0.0.0",
  "description": "1 on 1 project",
  "author": "Adrian Wrobel",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register -r dotenv-safe/config src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "postinstall": "npm run prestart:prod",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node -r dotenv-safe/config ./dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.3.1",
    "@nestjs/core": "^6.3.1",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.3.1",
    "@nestjs/platform-socket.io": "^6.4.0",
    "@nestjs/platform-ws": "^6.4.0",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/websockets": "^6.3.1",
    "axios": "^0.19.0",
    "dotenv-safe": "^6.1.0",
    "google-libphonenumber": "^3.2.2",
    "luxon": "^1.16.0",
    "mongoose": "^5.6.0",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "socket.io-redis": "^5.2.0",
    "swagger-ui-express": "^4.0.6",
    "typescript": "^3.5.2",
    "winston": "2.4.4"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/google-libphonenumber": "^7.4.17",
    "@types/jest": "^23.3.1",
    "@types/mongoose": "^5.3.20",
    "@types/node": "^10.7.1",
    "@types/socket.io": "^2.1.2",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
